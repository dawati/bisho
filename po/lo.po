#: ../data/bisho.desktop.in.h:1
msgid "Web Services"
msgstr "ການບໍລິການ ເວບ"

#: ../data/bisho.desktop.in.h:2
msgid "myzone & status"
msgstr "myzone ເເລະ ສະຖານະພາບ"

#: ../src/bisho-window.c:94
msgid "Launch site for more information."
msgstr "ເປີດເວບໄຊ້ທ໌ເພື່ອເອົາຂໍ້ມູນເພີ້ມ."

#: ../src/bisho-window.c:105 ../src/bisho-window.c:121
msgid "Username:"
msgstr "ຊື່ຜູ້ໃຊ້:"

#: ../src/bisho-window.c:127
msgid "Password:"
msgstr "ລະຫັດພ່ານ:"

#: ../src/bisho-window.c:197
msgid "Web Services Settings"
msgstr "ຕັ້ງຄ່າ ການບໍລິການ ເວບ"

#: ../src/bisho-window.c:204
msgid ""
"Set up your social networks to see new updates in myzone and the status "
"panel."
msgstr "ສ້າງຕັ້ງເຄື່ອຄ່າຍສັງຄົງເພື່ອເບີ່ງການປັບປຸງໃຫມ່ ໃນ ຫນ້າ myzone ເເລະ ສະຖານະພາບ"

#: ../src/bisho-window.c:225
#, c-format
msgid "%s login changed."
msgstr "%s ເຂົ້າລະບົບປ່ຽນ."

#: ../src/bisho-facebook-pane.c:110 ../src/bisho-oauth-pane.c:62
#: ../src/bisho-flickr-pane.c:65
#, c-format
msgid ""
"You'll need an account with %s and an Internet connection to use this web "
"service."
msgstr "ເຈົ້າຕ້ອງການປັນຊີກັບ %s ເເລະ ການເຊີ່ມຕໍ່ ອີນເຕິເເນດ ກ່ອນຈະໃຊ້ການບຊລິການເວບ."

#: ../src/bisho-facebook-pane.c:307 ../src/bisho-oauth-pane.c:222
#: ../src/bisho-flickr-pane.c:251
msgid "Log in pending"
msgstr "ລໍຖ້າ ເຂົ້າລະບົບ"

#: ../src/bisho-facebook-pane.c:308 ../src/bisho-oauth-pane.c:223
#: ../src/bisho-flickr-pane.c:252
msgid "Log me in"
msgstr "ໃຫ້ຂ້ອຍ ເຂົ້າລະບົບ"

#: ../src/bisho-facebook-pane.c:313 ../src/bisho-oauth-pane.c:228
#: ../src/bisho-flickr-pane.c:257
msgid "Log in pending..."
msgstr "ລໍຖ້າ ເຂົ້າລະບົບ..."

#: ../src/bisho-facebook-pane.c:314 ../src/bisho-oauth-pane.c:229
#: ../src/bisho-flickr-pane.c:258
msgid "Working..."
msgstr "ກໍາລັງທໍາງານ..."

#: ../src/bisho-facebook-pane.c:318
msgid "Press Continue to continue the log in."
msgstr "ກົດປູມ ຕໍ່ໄປ ເພື່ອເຂົ້າລະບົບ."

#: ../src/bisho-facebook-pane.c:319 ../src/bisho-oauth-pane.c:242
#: ../src/bisho-flickr-pane.c:263
msgid "Continue"
msgstr "ຕໍ່ໄປ"

#: ../src/bisho-facebook-pane.c:326 ../src/bisho-flickr-pane.c:270
#, c-format
msgid "Logged in as %s"
msgstr "ເຂົ້າລະບົບເເລ້ວເປັນເເບບ %s"

#: ../src/bisho-facebook-pane.c:330 ../src/bisho-oauth-pane.c:248
#: ../src/bisho-flickr-pane.c:274
msgid "Logged in"
msgstr "ເຂົ້າລະບົບເເລ້ວ"

#: ../src/bisho-facebook-pane.c:332 ../src/bisho-oauth-pane.c:249
#: ../src/bisho-flickr-pane.c:276
msgid "Log me out"
msgstr "ໃຫ້ຂ້ອຍ ອອກລະບົບ"

#: ../src/bisho-facebook-pane.c:396 ../src/bisho-oauth-pane.c:286
#: ../src/bisho-flickr-pane.c:333
msgid "<b>Status:</b>"
msgstr "<b>ສະຖານະພາບ:</b>"

#: ../src/bisho-oauth-pane.c:237
#, c-format
msgid "Once you have logged in to %s, press Continue."
msgstr "ເມື່ອເຈົ້າເຂົ້າລະບົບເເລ້ວທີ່ %s, ກົດປູມ ຕໍ່ໄປ."

#: ../src/bisho-flickr-pane.c:262
msgid "Once you have logged in to Flickr, press Continue."
msgstr "ເມື່ອເຈົ້າເຂົ້າລະບົບເເລ້ວທີ່ Flickr, ກົດປູມ ຕໍ່ໄປ."

