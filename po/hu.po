#: ../data/bisho.desktop.in.h:1
#: ../src/bisho-window.c:164
msgid "My Web Accounts"
msgstr "Webfiókok"

#. Please don't translate "myzone"
#: ../data/bisho.desktop.in.h:3
msgid "myzone & status"
msgstr "zónám és állapotom"

#: ../src/bisho-pane.c:86
msgid "Launch site for more information."
msgstr "Ugrás az oldalra a többi információért"

#: ../src/bisho-pane.c:90
#, c-format
msgid "<small>You'll need an account with %s and an Internet connection to use this web service.</small>"
msgstr "<small>Ezen webes szolgáltatás használatához szükséged lesz egy %s fiókra és internet kapcsolatra.</small>"

#: ../src/bisho-pane.c:204
#, c-format
msgid "Sorry, we can't log in to %s. %s"
msgstr "Bocsi, nem sikerült bejelentkezni ide: %s. %s"

#: ../src/bisho-pane.c:208
#, c-format
msgid "Sorry, we can't log in to %s"
msgstr "Bocsi, nem sikerült bejelentkezni ide: %s"

#: ../src/bisho-pane-username.c:57
#, c-format
msgid "%s login changed."
msgstr "%s bejelentkezés megváltozott."

#: ../src/bisho-window.c:74
#: ../src/bisho-window.c:80
msgid "Username:"
msgstr "Felhasználónév:"

#: ../src/bisho-window.c:82
msgid "Password:"
msgstr "Jelszó:"

#: ../src/bisho-window.c:188
msgid "Quit"
msgstr "Kilépés"

#. Please don't translate 'myzone'
#: ../src/bisho-window.c:199
msgid "Set up your social networks to see new updates in myzone and the status panel."
msgstr "Állítsd be kapcsolati hálódat, hogy láthasd a frissítéseket a zónádban és az állapot panelen."

#: ../panes/flickr.c:248
#: ../panes/oauth.c:307
msgid "Log me in"
msgstr "Bejelentkezem"

#: ../panes/flickr.c:252
#: ../panes/oauth.c:311
msgid "Connecting..."
msgstr "Csatlakozás..."

#: ../panes/flickr.c:256
#: ../panes/oauth.c:347
msgid "Log in succeeded. You'll see new items in a couple of minutes."
msgstr "Sikeres bejelentkezés. Az új elemek perceken belül megérkezik."

#: ../panes/flickr.c:259
#: ../panes/oauth.c:349
msgid "Log me out"
msgstr "Kijelentkezem"

#: ../panes/oauth.c:95
msgid "You don't seem to have a network connection, this won't work."
msgstr "Nem látok át a hálózati kapcsolaton. Szerintem nem működik."

#: ../panes/oauth.c:96
msgid "You could check that the computer's clock is correct."
msgstr "Ellenőrizd le, hogy a számítógép órája pontosan jár-e."

#: ../panes/oauth.c:97
msgid "You could try again."
msgstr "Próbálja meg újra."

#: ../panes/oauth.c:320
#, c-format
msgid "Once you have logged in to %s, press Continue."
msgstr "Ha már bejelentkeztél a %s fiókra, nyomj Tovább-ot."

#: ../panes/oauth.c:325
#: ../panes/oauth.c:342
msgid "Continue"
msgstr "Tovább"

#: ../panes/oauth.c:337
#, c-format
msgid "Once you have logged in to %s, enter the code they give you and press Continue."
msgstr "Egyszer már bejelentkeztél a %s fiókra. Add meg a kódot és nyomj Tovább-ot."

#: ../panes/oauth.c:446
msgid "Code:"
msgstr "Kód:"

