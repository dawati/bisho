#: ../data/bisho.desktop.in.h:1
msgid "Web Services"
msgstr "Servizi web"

#: ../data/bisho.desktop.in.h:2
msgid "myzone, status & more"
msgstr "myzone, stato e altro"

# (ndt) è una specie di pulsante/collegamento
#: ../src/bisho-window.c:94
msgid "Launch site for more information"
msgstr "Apri il sito web per maggiori informazioni"

#: ../src/bisho-window.c:106 ../src/bisho-window.c:122
msgid "Username:"
msgstr "Nome utente:"

#: ../src/bisho-window.c:128
msgid "Password:"
msgstr "Password:"

#: ../src/bisho-window.c:198
msgid "Web Services Settings"
msgstr "Impostazioni dei servizi web"

#: ../src/bisho-window.c:205
msgid ""
"Set up your social networks to see new updates in myzone and the status "
"panel."
msgstr ""
"Impostare i social network a cui si partecipa per visualizzare aggiornamenti "
"all'interno della myzone e nel pannello di stato."

# (ndt) un po' lunga e libera, ma c'è spazio
# si vede quando si modificano le credenziali
# di un servizio web
#: ../src/bisho-window.c:226
#, c-format
msgid "%s login changed."
msgstr "Informazioni di accesso di %s modificate."

#: ../src/bisho-oauth-pane.c:62 ../src/bisho-flickr-pane.c:65
#, c-format
msgid ""
"You'll need an account with %s and an Internet connection to use this web "
"service."
msgstr ""
"Sono necessari un account con %s e una connessione a Internet per utilizzare "
"questo servizio web."

#: ../src/bisho-oauth-pane.c:222 ../src/bisho-flickr-pane.c:251
msgid "Log in pending"
msgstr "Accesso in corso"

# (ndt) pulsante
#: ../src/bisho-oauth-pane.c:223 ../src/bisho-flickr-pane.c:252
msgid "Log me in"
msgstr "Accedi"

#: ../src/bisho-oauth-pane.c:228 ../src/bisho-flickr-pane.c:257
msgid "Log in pending..."
msgstr "Accesso..."

#: ../src/bisho-oauth-pane.c:229 ../src/bisho-flickr-pane.c:258
msgid "Working..."
msgstr "In lavorazione..."

#: ../src/bisho-oauth-pane.c:237
#, c-format
msgid "Once you have logged in to %s, press Continue."
msgstr "Una volta eseguito l'accesso a %s, fare clic su «Continua»."

# (ndt) pulsante
#: ../src/bisho-oauth-pane.c:242 ../src/bisho-flickr-pane.c:263
msgid "Continue"
msgstr "Continua"

#: ../src/bisho-oauth-pane.c:248 ../src/bisho-flickr-pane.c:274
msgid "Logged in"
msgstr "Accesso effettuato"

#: ../src/bisho-oauth-pane.c:249 ../src/bisho-flickr-pane.c:276
msgid "Log me out"
msgstr "Disconnetti"

#: ../src/bisho-oauth-pane.c:286 ../src/bisho-flickr-pane.c:333
msgid "<b>Status:</b>"
msgstr "<b>Stato:</b>"

#: ../src/bisho-flickr-pane.c:262
msgid "Once you have logged in to Flickr, press Continue."
msgstr "Una volta eseguito l'accesso a Flickr, fare clic su «Continua»."

#: ../src/bisho-flickr-pane.c:270
#, c-format
msgid "Logged in as %s"
msgstr "Accesso effettuato come %s"

#~ msgid "Press Continue to continue the log in."
#~ msgstr "Premere «Continua» per continuare l'accesso."
