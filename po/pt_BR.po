#: ../data/bisho.desktop.in.h:1 ../src/bisho-window.c:39
#: ../src/bisho-cc-panel.c:69
msgid "My Web Accounts"
msgstr "Minhas contas web"

#. Please don't translate "myzone"
#: ../data/bisho.desktop.in.h:3
msgid "myzone & status"
msgstr "myzone e status"

#: ../src/bisho-pane.c:82
msgid "Launch site for more information."
msgstr "Visite o site para maiores informações."

#: ../src/bisho-pane.c:86
#, c-format
msgid "<small>You'll need an account with %s and an Internet connection to use this web service.</small>"
msgstr "<small>Você precisará de uma conta com %s e uma conexão a Internet para usar este serviço web.</small>"

#: ../src/bisho-pane.c:223
#, c-format
msgid "Sorry, we can't log in to %s. %s"
msgstr "Sinto muito, não foi possível conectar no  %s. %s"

#: ../src/bisho-pane.c:227
#, c-format
msgid "Sorry, we can't log in to %s"
msgstr "Sinto muito, não foi possível conectar no %s"

#: ../src/bisho-pane-username.c:69
#, c-format
msgid "Log in succeeded. You'll see new things from %s in a couple of minutes."
msgstr "Conectado. Você verá novos itens do %s  em alguns minutos."

#: ../src/bisho-pane-username.c:149
msgid "Log in"
msgstr "Fazer login"

#: ../src/bisho-window.c:65
msgid "Quit"
msgstr "Sair"

#: ../src/bisho-frame.c:75 ../src/bisho-frame.c:81
msgid "Username:"
msgstr "Nome do usuário:"

#: ../src/bisho-frame.c:83
msgid "Password:"
msgstr "Senha:"

#. Please don't translate 'myzone'
#: ../src/bisho-frame.c:214
msgid "Set up your social networks to see new updates in myzone and the status panel."
msgstr "Configure suas redes sociais para ver novas atualizações no myzone e no painel de status."

#: ../panes/flickr.c:286 ../panes/oauth.c:307
msgid "Log me in"
msgstr "Conectar"

#: ../panes/flickr.c:290 ../panes/oauth.c:311
msgid "Connecting..."
msgstr "Conectando..."

#: ../panes/flickr.c:291 ../panes/oauth.c:325 ../panes/oauth.c:342
msgid "Continue"
msgstr "Continuar"

#: ../panes/flickr.c:295 ../panes/oauth.c:347
msgid "Log in succeeded. You'll see new items in a couple of minutes."
msgstr "Conectado. Você verá novos itens em alguns minutos."

#: ../panes/flickr.c:298 ../panes/oauth.c:349
msgid "Log me out"
msgstr "Desconectar"

#: ../panes/oauth.c:95
msgid "You don't seem to have a network connection, this won't work."
msgstr "Você parece não ter uma conexão de rede, isto não funcionará."

#: ../panes/oauth.c:96
msgid "You could check that the computer's clock is correct."
msgstr "Você poderia verificar se o relógio do computador está correto."

#: ../panes/oauth.c:97
msgid "You could try again."
msgstr "Você poderia tentar de novo."

#: ../panes/oauth.c:320
#, c-format
msgid "Once you have logged in to %s, press Continue."
msgstr "Uma vez conectado no %s, pressione Continuar."

#: ../panes/oauth.c:337
#, c-format
msgid "Once you have logged in to %s, enter the code they give you and press Continue."
msgstr "Uma vez conectado no %s, digite o código fornecido e pressione Continuar."

#: ../panes/oauth.c:446
msgid "Code:"
msgstr "Código:"

#: ../src/bisho-pane-username.c:114
#, c-format
msgid "Log out succeeded. All trace of %s has been removed from your computer."
msgstr "Êxito no logout. Tudo do %s foi removido do seu computador."

